{
  "name": "edutvum-exam",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch",
    "test:ci": "ng test --code-coverage --single-run",
    "e2e": "ng e2e",
    "dev": "ng serve -c dev >> output-ng.txt 2>&1 &",
    "firebase": "firebase emulators:start --only database --import=../.devrtdb --export-on-exit >> output-db.txt 2>&1 &",
    "fshttp": "fshttp -d ../.uploads >> output-fs.txt 2>&1 &",
    "devstop": "fuser -k 8000/tcp ; fuser -k 4000/tcp ; fuser -k 4500/tcp ; fuser -k 9000/tcp ; fuser -k 4200/tcp",
    "devstart": "rm output*.txt && npm run firebase && npm run fshttp && npm run dev",
    "stage": "ng serve -c stage",
    "stage-tt2": "ng serve -c stage-tt2",
    "prod": "ng serve --configuration production",
    "build": "ng build --configuration production",
    "lint": "ng lint",
    "deploy": "ng build --configuration production && firebase deploy --only hosting",
    "export": "firebase database:get / edutvum-exam > cmd-line/edutvum-exam-export-$(date +%Y%m%d_%H).json",
    "verify": "tsc cmd-line/verify.ts && node cmd-line/verify.js cmd-line/edutvum-exam-import.json",
    "backup": "gsutil -m cp -r gs://edutvum-exam.appspot.com/ .",
    "postinstall": "ngcc --properties es2015 browser module main --first-only"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.1",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.1",
    "@angular/compiler": "^17.3.1",
    "@angular/core": "^17.3.1",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.3.1",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^17.3.1",
    "@angular/platform-browser-dynamic": "^17.3.1",
    "@angular/platform-server": "^17.3.1",
    "@angular/router": "^17.3.1",
    "@angular/service-worker": "^17.3.1",
    "angularfire2": "^5.2.3",
    "bootstrap": "^3.3.7",
    "core-js": "^2.6.12",
    "firebase": "^7.24.0",
    "firebase-admin": "^8.13.0",
    "jsonfile": "^5.0.0",
    "jsonschema": "^1.4.0",
    "katex": "^0.11.1",
    "moment": "^2.29.1",
    "mudder": "^1.0.10",
    "ngx-pagination": "^6.0.3",
    "ngx-quill": "^25.1.2",
    "npm": "^6.14.11",
    "popper.js": "^1.16.0",
    "quill": "^2.0.0-rc.4",
    "rxjs": "^7.8.1",
    "rxjs-compat": "^6.6.3",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.1",
    "@angular/language-service": "^17.3.1",
    "@angular/pwa": "^0.901.14",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^13.13.42",
    "axios": "^0.19.2",
    "codelyzer": "^5.2.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jsonpp": "^1.0.2",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "puppeteer": "^3.3.0",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "^8.10.2",
    "tsickle": "^0.35.0",
    "tslib": "^1.14.1",
    "tslint": "^6.1.3",
    "typescript": "^5.4.3",
    "webpack": "^4.46.0"
  }
}