    "lib": [
      "es2016",
      "dom"
    ],
    "downlevelIteration": true,
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "ES2015",
    "typeRoots": [
      "../node_modules/@types",
    ],
    "paths": {
      "iupac": [
        "../dist/iupac"
      ]
    }
  },
  "files": [
    "main.ts",
    "polyfills.ts"
  ]
}
